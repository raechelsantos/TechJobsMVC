    <div class="container body-content">
    <h1>@ViewBag.title</h1>
    @{
        
        List<Job> jobs = ViewBag.jobs;
        if (ViewBag.jobs != null)
        {
            foreach (Job job in jobs)
            {
                <table class="job-listing">
                    <tr><td>ID: @job.Id</td></tr>
                    <tr><td>Name: @job.Name</td></tr>
                    <tr><td>Employer: @job.Employer</td></tr>
                    <tr><td>Location: @job.Location</td></tr>
                    <tr><td>Position Type: @job.PositionType</td></tr>
                    <tr><td>Skill: @job.CoreCompetency</td></tr>
                </table>
            }

