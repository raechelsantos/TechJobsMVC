<div class="container body-content">
    <h1>@ViewBag.title</h1>
</div>

    @{
        //TODO #1: Use a loop to display job results in a table.
        List<Job> jobs = ViewBag.jobs;
        if (ViewBag.jobs != null)
        {
                <table class="job-listing">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Employer</th>
                        <th>Location</th>
                        <th>Position Type</th>
                        <th>Core Competency</th>
                    </tr>
                </table>
        }

        foreach (var job in jobs)
        {
                <tr>
                    <td>@job.Id</td>
                    <td>@job.Name</td>
                    <td>@job.Employer</td>
                    <td>@job.Location</td>
                    <td>@job.PositionType</td>
                    <td>@job.CoreCompetency</td>
                </tr>
        }
     }
